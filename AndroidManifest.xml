<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vorsk.crossfitr"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".CrossFitrActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".WorkoutsActivity"
			android:label="@drawable/button_workout" >
		</activity>
		<activity android:name=".AllActivity"
			android:label="@string/all_label" >
		</activity>
		<activity android:name=".WodActivity"
			android:label="@string/wod_label" />
		<activity android:name=".GirlsActivity"
			android:label="@string/girl_label" />
		<activity android:name=".HeroesActivity"
			android:label="@string/hero_label" />
		<activity android:name=".CustomActivity"
			android:label="@string/custom_label" />
    </application>

</manifest>